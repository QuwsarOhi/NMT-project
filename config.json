{
    "device": "cuda",
    
    "dataset": {
        "batch_size": 64,
        "num_workers": 2
    },

    "model": {
        "freeze_till": 116
    },

    "trainer": {
        "limit_train_batches": 1.0,
        "max_epochs": 500,
        "deterministic": true,
        "accelerator": "gpu",
        "check_val_every_n_epoch": 1,
        "precision": "16",
        "enable_progress_bar": false,
        "default_root_dir": "./train_logs",
        "enable_checkpointing": true
    }
}